ve/cr xerr([np]) r

fnam='inc/v_poltr.inc'
if $FEXIST([fnam]) then
  shell rm -f [fnam]
endif
inam='      INTEGER MXWIEN'
fmess [inam] [fnam]
inam='      PARAMETER (MXWIEN='//[np]//')'
fmess [inam] [fnam]
inam='      VECTOR ATANG('//[np]//',2,2)'
fmess [inam] [fnam]
inam='      VECTOR ATANG1('//[np]//',2)'
fmess [inam] [fnam]
inam='      VECTOR ANGLE('//[np]//')'
fmess [inam] [fnam]

fp1='S'
fp2='S'
win1=1
win2=1

if [plot]=0 then
  fp1='0'
  fp2='0'
  win1=0
  win2=0
elseif [plot]=1 then
  fp1='S'
  fp2='0'
  win1=1
  win2=0
elseif [plot]=2 then
  fp1='0'
  fp2='S'
  win1=0
  win2=1
elseif [plot]=3 then
  fp1='S'
  fp2='S'
  win1=1
  win2=1
endif

ymax=1.1
if [ptran]=2 then
  ymax=0.1
endif

if [hall]=1 .AND. [aver]>0 then
  ja1=[aver]
  ja2=[aver]
  if [aver]>2 then
    ja1=1
    ja2=2
  endif
  do i=1,[np]
    sa=0
    jj=0
    do iang=[ja1],[ja2]
      jj=[jj]+1
      do j=1,2
        at=atang([i],[j],[iang])
        dt=tangad([j])
        at1=$SIGMA([at]+[dt])
        as=asym1([i],[j],[iang])
        as=$SIGMA([as]*COS([at]*3.14159/180.)/COS([at1]*3.14159/180.))
        ve/in asym1([i],[j],[iang]) [as]
        ve/in atang1([i],[j]) [at1]
      enddo
      a1=asym1([i],1,[iang])
      a2=asym1([i],2,[iang])
      e1=easym1([i],1,[iang])
      e2=easym1([i],2,[iang])
      if [ptran]=2 then
        a=$SIGMA(([a1]-[a2])/2.)
      else
        a=$SIGMA(([a1]+[a2])/2.)
      endif
      e=$SIGMA(SQRT(([e1]**2+[e2]**2)/2.))
      sa=$SIGMA([sa]+[a])
    enddo
    sa=$SIGMA([sa]/[jj])
    ve/in asymm([i]) [sa]
    ve/in easymm([i]) [e]
  enddo
endif

mess win: [win1] [win2] [fp1] [fp2]

i1=1
i2=[np]

*  ---  select a given Wien range
j=0
do i=1,[np]
  a=angle([i])
  if [a]<[w1] .OR. [a]>[w2] then
     ve/in easymm([i]) 0.1
  endif
enddo

*  ---  remove the RF data (unless required) (leave DC only)
if [rf]=0 then
  do i=1,[np]
    a=irf([i])
    if [a]>0.5 then
       mess [i]
       ve/in easymm([i]) 0.1
    endif
  enddo
endif

if [win1]>0 then
  if [same]=0 then
    null -120 120 -[ymax] [ymax]
    text -100 -0.8 [mtxt] 0.25 0. !
    atit 'Angle ' 'Beam Polarization (preliminary)'
*    atit 'Angle ' 'Beam Polarization '
    set dmod 3
    line -120 0 120 0
    set dmod 1
  endif
  gr/hpl/err angle asymm eangle easymm [np] 20 0.1
endif
set csiz 0.55

set dmod 1
ve/cr par(10) r 0.7 0. 1.
ve/in par(1:3) 0.7 0.  1. 

ve/in ipfit(1) 1

set txci 4
ve/fit angle([i1]:[i2]) asymm([i1]:[i2]) easymm([i1]:[i2]) wave_tr.f [fp1] [fitpar] par
set txci 1
a=par(1)
b=par(2)
period=par(3)
b=-[b]
ve/in ipfit(1) 0
*mess a,b,c [a] [b] 

if [win1]>0 then
*  f/pl [a]*cos(([b]+x)/180*3.1415*[period]) -120 120 s
  f/pl wave_tr.f -120 120 s
endif
* exitm

if [win2]=0 then
  goto eend
endif

do ip=1,[np]
  x=angle([ip])
  x=$RSIGMA([x])
  ex=eangle([ip])
  ep0=easymm([ip])
*  ep=$SIGMA([a]*SIN(([x]+[b])*3.1415/180.*[period])*[ex]*3.1415/180.*[period])
  cmn='wave_tr.f('//[x]//')'
  y1=$CALL([cmn])
  x=$RSIGMA([x]+1.)
  cmn='wave_tr.f('//[x]//')'
  y2=$CALL([cmn])
  ep=$SIGMA([ex]*ABS([y2]-[y1])/1.)

*  mess [a] [b] [ex] [ep] [ep0]
  ep1=$SIGMA(SQRT([ep]*[ep]+[ep0]*[ep0]))
*  mess [a] [b] [ex] [ep] [ep0] [ep1] [x]
  mess [ex] [ep] [ep0] [ep1] [x]
  ve/in easymm([ip]) [ep1]
enddo

set csiz 0.3
  if [same]=0 then
    null -120 120 -[ymax] [ymax]
*    atit 'Angle ' 'Beam Polarization (preliminary)'
    atit 'Angle ' 'Beam Polarization '
    set dmod 3
    line -120 0 120 0
    set dmod 1
  endif
  gr/hpl/err angle asymm eangle easymm [np] 20 0.1
endif
set csiz 0.55

ve/in par(1:3)  0.7 0. 1.

set dmod 1

ve/in ipfit(1) 1
set txci 4
ve/fit angle([i1]:[i2]) asymm([i1]:[i2]) easymm([i1]:[i2]) wave_tr.f [fp2] [fitpar] par
*a=par(1)
*b=par(2)
*period=par(3)
*b=-[b]
ve/in ipfit(1) 0

text -100 -0.8 [mtxt] 0.25 0. !
*  f/pl [a]*cos(([b]+x)/180*3.1415*[period]) -120 120 s
f/pl wave_tr.f -120 120 s

eend:

opt stat
set fcol 1
set txci 1
set hcol 1
set pcol 1
set xcol 1
set ycol 1
set csiz 0.4

*selnt 1
*text 10  0.5 'Figure 4. Results of spin precession measurement with five polarimeters' 0.3 0. C 
*selnt 10

RETURN





