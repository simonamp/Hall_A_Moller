MACRO set_magp11  e0=6.0
* calculation of moller settings for e0(GeV)
ve/de *
*****   Q1   *****
 sq1=$sigma(5.279-4.7125*[e0]+1.5838*[e0]**2-0.2014*[e0]**3-0.14876E-01*[e0]**4)
 sq11=$sigma(0.56933E-02*[e0]**5-0.47019E-03*[e0]**6+0.12885E-04*[e0]**7)
 q1=[sq1]+[sq11]
 qq1=[q1]*1000.
 gl1=[qq1]*7.204
* cur1=[q1]*46.29
 c1=$sigma(-0.63599+0.38375E-01*[qq1]+0.3352E-07*[qq1]**2+0.14146E-09*[qq1]**3)
 cur11=$sigma([c1]-0.55637E-15*[qq1]**4)
 pr1=40.076
 pr2=-8.12331E-05
   if [q1]<0.0 then
      pr2=-1.*[pr2]
   endif
 cur1=$sigma([pr1]*[q1]*(1.-[pr2]*[q1]**4))
*****   Q2  ******
 q2=$sigma(-1.8588+2.584*[e0]-0.42839*[e0]**2+0.15816E-02*[e0]**3+0.12176E-02*[e0]**4) 
 qq2=[q2]*1000.
 gl2=[qq2]*8.804 
* cur2=[q2]*50.361
 c2=$sigma(-1.2205+0.49242E-01*[qq2]-0.65588E-07*[qq2]**2)
 c21=$sigma([c2]-0.36348E-11*[qq2]**3+0.26204E-14*[qq2]**4)
 cur22=$sigma([c21]+0.1094E-17*[qq2]**5)
 pr1=49.053
 pr2=-0.201075E-04
   if [q2]<0.0 then
      pr2=-1.*[pr2]
   endif
 cur2=$sigma([pr1]*[q2]*(1.-[pr2]*[q2]**4))
*****   Q3   *****
    if [e0]<2.5 then
      q3=0.0
        elseif [e0]<4.0 then
           q3=$sigma(0.66771E-01-0.90703E-01*[e0]+0.22826E-01*[e0]**2+0.11067E-02*[e0]**3) 
             else
               sq3=$sigma(14.843-13.799*[e0]+4.5182*[e0]**2-0.65623*[e0]**3)
               sq33=$sigma(0.4443E-01*[e0]**4-0.11474E-02*[e0]**5)
               q3=[sq3]+[sq33]
*  q3=$sigma(-7.9152+3.1003*[e0]-0.31932*[e0]**2+0.10859E-01*[e0]**3)
    endif
   qq3=[q3]*1000.
   gl3=[qq3]*7.1792
*   cur3=[q3]*48.807
 c3=$sigma(-1.315+0.4101E-01*[qq3]+0.15644E-05*[qq3]**2-0.4145E-09*[qq3]**3)
 cur33=$sigma([c3]-0.36829E-13*[qq3]**4+0.14817E-16*[qq3]**5)
 pr1=41.4001
 pr2=-0.00012339
   if [q3]<0.0 then
      pr2=-1.*[pr2]
   endif
 cur3=$sigma([pr1]*[q3]*(1.-[pr2]*[q3]**4))
*****   Q4   *****
 sq4=$sigma(2.1511-3.3884*[e0]+2.2896*[e0]**2-0.67665*[e0]**3+0.1035*[e0]**4)
 sq44=$sigma(-0.76225E-02*[e0]**5+0.21254E-03*[e0]**6)
 q4=[sq4]+[sq44] 
 qq4=[q4]*1000.
 gl4=[qq4]*7.0977
* cur4=[q4]*47.975
 c4=$sigma(-1.315+0.4101E-01*[qq4]+0.15644E-05*[qq4]**2-0.4145E-09*[qq4]**3)
 cur44=$sigma([c4]-0.36829E-13*[qq4]**4+0.14817E-16*[qq4]**5)
 pr1=40.584
 pr2=-0.000107222
   if [q4]<0.0 then
      pr2=-1.*[pr2]
   endif
 cur4=$sigma([pr1]*[q4]*(1.-[pr2]*[q4]**4))
*****   Dipole   *****
* sdp=$sigma(-0.24196+2.1987*[e0]-0.58477*[e0]**2+0.20756*[e0]**3-0.34884E-01*[e0]**4)
* sdpp=$sigma(0.27558E-02*[e0]**5-0.83668E-04*[e0]**6)
* dp=[sdp]+[sdpp]
 sdp=$sigma(-0.418647E-01+1.58469*[e0]-0.388859E-01*[e0]**2+0.809835E-02*[e0]**3)
 dp=$sigma([sdp]-0.622359E-03*[e0]**4)
 bdl=[dp]*114993.1
* curdp=[dp]*25.67
 cdp=$sigma(-0.47975E-03+36.969*[dp])

 c1=([cur1]+[cur11])/2.
 c2=([cur2]+[cur22])/2.
 c3=([cur3]+[cur33])/2.
 c4=([cur4]+[cur44])/2.

 mess Eb=[e0]GeV
 mess .       Q1          Q2         Q3          Q4         Dipole
 mess  kG   [q1]      [q2]       [q3]       [q4]     [dp]     
 mess  kG*m [gl1]    [gl2]    [gl3]     [gl4]     [bdl]    
 mess  A    [cur1]    [cur2]     [cur3]     [cur4]      
 mess  A    [cur11]    [cur22]    [cur33]     [cur44]     [cdp] 
 mess  A    [c1]    [c2]    [c3]     [c4]     [cdp]  Aver 
RETURN
