* Hall A Moller spin dance 31 August 2009
* runs 13465-13483

* ebeam=3.4839
 ebeam=3.4812
 inject=0.064125
 pass=3

 set pmci 1
 fort/fil 56 spin_prec_131.ps
 meta 56 -112

 np=4
