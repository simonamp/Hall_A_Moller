MACRO spin_prec_19 
*
* Hall A Moller spin dance 29 Feb 2016
* ebeam - beam energy in GeV
* inject - injectro energy in Gev
* pass - number of pass
 opt ndat
 ebeam=8.846
 inject=0.123
 pass=4
 opt nbox
 opt ndat

 set pmci 1
 fort/fil 56 spin_prec_19.ps
 meta 56 -112
*F lipper vertical 0.0 Solenoid 0.0
 np=4
 ve/cr exx([np])  r 
 ve/cr wang([np]) r -54.0  -90.0  25.0  -88.0
* ve/cr bpol([np]) r -68.02 -87.14 38.89 -87.66
 ve/cr bpol([np]) r -67.70 -86.40 38.49 -86.95
 ve/cr bpol1([np]) r 
 ve/cr ebpol([np]) r 0.22   0.19  0.22  0.20
 set fit 1111 
 ve/cr ipfit(2) i
 ve/in ipfit 0 0
 ve/cr par(10) r
 zone 
 set csiz 0.5
 null -100. 100.  -90.0  90. 
 ve/fit wang bpol ebpol wave.f s  2  par
 fu/pl wave.f -100 40 s
 gr/hpl/err wang bpol exx ebpol [np] 20 0.2
 atitle 'Wien Angle, deg' 'Beam Polarization, ru'

 selnt 1
 text 14.0 20.5 'Moller results for Spin Dance 02/29/2016' 0.4 0 C
 selnt 10
* a=par(1)
* b=par(2)
* mess a=[a] b=[b]

 close 56

RETURN

